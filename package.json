{
  "name": "ryodev",
  "displayName": "RyoDev",
  "description": "The extension to run \"npm run dev\" and \"npm run serve\"",
  "version": "0.0.52",
  "publisher": "Budi Imam Prasetyo",
  "engines": {
    "vscode": "^1.89.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/budi-imam-prasetyo/RyoDev"
  },
  "icon": "images/RyoDev.png",
  "contributes": {
    "commands": [
      {
        "command": "extension.toggleRyoDevServe",
        "title": "Ryo: Toggle Dev Serve"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.1",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
